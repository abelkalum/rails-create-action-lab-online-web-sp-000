<!-- Create your form here -->
<%= form_tag students_path do %>
  <label>Student name:</label><br>
  <%= text_field_tag :name %><br>

  <label>First name</label><br>
  <%= text_area_tag :first_name %><br>
  
  <label>Last name</label><br>
  <%= text_area_tag :last_name %><br>

  <%= submit_tag "Submit" %>
<% end %>